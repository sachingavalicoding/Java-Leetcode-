class Ugly 
{
	public static void main(String[] args) {
		int n = 14;
		
		
		while(n > 1){
            if(n % 2 == 0){
                n /= 2;
            }
            else if(n % 3 == 0){
                n /= 3;
            }
            else if(n % 5 == 0){
                n /= 5;
            }
            else{
                System.out.println(" Not a ugly number");
				break;
            }
        }
        System.out.println(" ugly number");
	}
}
