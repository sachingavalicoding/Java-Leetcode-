class PerfectNum 
{
	public static void main(String[] args) 
	{
		int num = 6;
		int factSum = 0;
		for(int i = 1; i < num; i++){
			if(num % i == 0){
				factSum += i;
			}
			System.out.print( i + " ");
			
		}
		System.out.println(factSum == num ? " Number " + factSum + " is Perfect Number " : " Not a Perfect number ");
	}
}
